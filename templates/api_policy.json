{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Principal": {
          "AWS": ["arn:aws:iam::${accountid}:root",
            "arn:aws:iam::${accountid}:role/Admin"
          ]
        },
        "Action": "execute-api:Invoke",
        "Resource": "arn:aws:execute-api:${region}:${accountid}:*/*/POST/resource"
      }
    ]
  }